[alias]
   delete = rm
   dts = "difftool --staged"
   fpush = "push --force-with-lease"
   log1 = "log --oneline"
   meld = mergetool
   pull-up = "!f() { git remote update && git pull; }; f"
   st = "status"
   dt = "difftool"
   stcached = "diff --name-only --cached"
   lol=log --decorate --pretty=oneline --abbrev-commit
   lola=log --graph --decorate --pretty=oneline --abbrev-commit --all

[user]
	name = Joe Ferraro
   email = joe@ironsmithventures.com

[diff]
   tool = tkdiff

[merge]
   tool = meld

[difftool]
   prompt = false

[core]
   compression = 1
[init]
	defaultBranch = master
